<div class="row">
  <div class="col-lg-12 col-width">
    <% for (let games in gameList) { %>
      <% if (gameList[games].players < 2) { %>
        <%= console.log(games, gameList[games]); %>
        <div class="queue-item">

        <div class="col-lg-6 game-name-col">
          <span id="game-name">Goofspiel</span>
        </div>
        <div class="col-lg-3 num-players-col">
          <span id="num-players"><%= gameList[games].players %>/2</span>
        </div>
        <div class="col-lg-3 join-col">
          <a class="join btn btn-primary" href="/join/<%= games %>">Join</a>
        </div>
      </div> <!-- closes queue-item -->
      <% } %>
    <% } %>
  </div> <!-- closes col-lg-12 -->
</div>